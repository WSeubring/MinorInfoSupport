{
  "version": "1.0.0-*",
  "buildOptions": {
    "emitEntryPoint": true,
    "debugType": "portable"
  },
  "dependencies": {
    "HAZ.FeBestellingen.Infrastructure": "1.0.0-*",
    "Microsoft.Extensions.Configuration": "1.0.0",
    "Microsoft.Extensions.Configuration.Abstractions": "1.1.0",
    "Microsoft.Extensions.Configuration.EnvironmentVariables": "1.0.0",
    "Microsoft.Extensions.Configuration.Json": "1.0.0",
    "Microsoft.NETCore.App": {
      "type": "platform",
      "version": "1.0.1"
    },
    "Serilog": "2.3.0",
    "Serilog.Extensions.Logging": "1.3.1",
    "Serilog.Sinks.File": "3.1.1",
    "Serilog.Sinks.RollingFile": "3.2.0",
    "Serilog.Settings.Configuration": "2.2.0",
    "Microsoft.Extensions.Logging": "1.1.0",
    "Microsoft.Extensions.Logging.Debug": "1.0.0",
    "Microsoft.Extensions.Logging.Console": "1.0.0",
    "Microsoft.Extensions.DependencyInjection": "1.1.0",
    "Pomelo.EntityFrameworkCore.MySql": "1.1.0",
    "InfoSupport.WSA.Auditlog.Shared": "1.0.1"
  },
  "frameworks": {
    "netcoreapp1.0": {
      "imports": "dnxcore50"
    }
  },
  "publishOptions": {
    "include": [
      "docker-compose.yml",
      "Dockerfile",
      ".dockerignore",
      "appsettings.json"
    ]
  },
  "scripts": {
    "postcompile": [
      "dotnet publish --no-build -f \"netcoreapp1.0\" -r \"debian.8.x64\" -c debug -o \"bin\\debug\\netcoreapp1.0\\publish\""
    ]
  }
}