<h1>Zoek Onderhoudsopdracht</h1>

<form class="" asp-action="Zoek" asp-controller="OnderhoudsOpdrachten" method="post">
    <div class="row">
        <div class="form-group col-sm-6">
            <label for="kenteken">Kenteken:</label>
            <input type="text" class="form-control" id="kenteken" name="kenteken" placeholder="1-ABC-23">
            <br />
        </div>
        <div class="form-group col-sm-6">
            <button type="submit" class="btn btn-primary">Zoek opdracht</button>
        </div>
    </div>
</form>
